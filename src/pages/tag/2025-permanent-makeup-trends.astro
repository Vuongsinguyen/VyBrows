---
// Redirect page for old blog/tag URLs
export const prerender = true;
---

<script>
  // Client-side redirect for old URLs
  if (typeof window !== 'undefined') {
    window.location.replace('/news/');
  }
</script>

<html>
<head>
  <meta http-equiv="refresh" content="0;url=/news/">
  <link rel="canonical" href="/news/">
  <title>Redirecting to VYBROWS ACADEMY News...</title>
</head>
<body>
  <p>Redirecting to <a href="/news/">VYBROWS ACADEMY News</a>...</p>
  <script>
  window.location.href = '/news/';
  </script>
</body>
</html>